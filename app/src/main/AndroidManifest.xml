<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cyclistassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_bike"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_bike_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UpdatingServices"></activity>
        <activity
            android:name=".UpdatingService"
            android:label="@string/title_activity_updating_service"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ViewingServices" />
        <activity android:name=".AddingServices" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>